{
  "name": "nustack-vscode",
  "displayName": "NuStack",
  "description": "NuStack language support for VSCode",
  "version": "1.0.0",
  "publisher": "dylanrenwick",
  "repository": "https://github.com/dylanrenwick/nustack-vscode.git",
  "homepage": "https://github.com/dylanrenwick/nustack-vscode#readme",
  "license": "MIT",
  "engines": {
    "vscode": "^1.34.0"
  },
  "bugs": {
    "url": "https://github.com/dylanrenwick/nustack-vscode/issues"
  },
  "contributes": {
    "languages": [
      {
        "id": "nustack",
        "extensions": [ ".ns" ]
      }
    ],
    "grammars": [
      {
        "language": "nustack",
        "scopeName": "source.nustack",
        "path": "./syntaxes/nustack.tmGrammar.json"
      }
    ]
  }
}
